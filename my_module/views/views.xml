<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="Car_show">
      <field name="name">MyModule list</field>
      <field name="model">employee.data</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="id"/>
          <field name="phone"/>
           <field name="start_date"/>
           <field name="end_date"/>
        </tree>
      </field>
    </record>

    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="employee_action_window">
      <field name="name">MyModule window</field>
      <field name="res_model">employee.data</field>
      <field name="view_mode">tree,form</field>
    </record>

