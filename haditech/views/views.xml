<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="haditech.list">
      <field name="name">haditech list</field>
      <field name="model">employees.data</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="email"/>
          <field name="phone"/>
        </tree>
      </field>
    </record>

    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="employee.action_window">
      <field name="name">haditech window</field>
      <field name="res_model">employees.data</field>
      <field name="view_mode">tree,form</field>
    </record>


